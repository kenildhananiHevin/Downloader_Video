        <activity
            android:name=".activity.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".instagram.story.InstagramStoryActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".instagram.story.InstaStoryVideoShowActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".instagram.story.InstaStoryImageShowActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
